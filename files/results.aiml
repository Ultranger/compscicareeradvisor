<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">

    <category>
        <pattern>A</pattern>
        <template>
            <condition name="question" value="9">
                <think><set name="jobA" add="1"></set></think>
                Thank you for answering all the questions. Let me recommend a job for you...
                <think>
                    <condition name="jobA" value="MAX">
                        Based on your responses, Software Developer might be the best fit for you!
                    </condition>
                    <condition name="jobB" value="MAX">
                        Based on your responses, Data Analyst might be the best fit for you!
                    </condition>
                    <condition name="jobC" value="MAX">
                        Based on your responses, Cybersecurity Analyst might be the best fit for you!
                    </condition>
                    <condition name="jobD" value="MAX">
                        Based on your responses, AI/ML Engineer might be the best fit for you!
                    </condition>
                    <condition name="jobE" value="MAX">
                        Based on your responses, IT Support Specialist might be the best fit for you!
                    </condition>
                </think>
            </condition>
        </template>
    </category>

